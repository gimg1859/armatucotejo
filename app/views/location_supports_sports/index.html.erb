<p id="notice"><%= notice %></p>

<h1>Location Supports Sports</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @location_supports_sports.each do |location_supports_sport| %>
      <tr>
        <td><%= link_to 'Show', location_supports_sport %></td>
        <td><%= link_to 'Edit', edit_location_supports_sport_path(location_supports_sport) %></td>
        <td><%= link_to 'Destroy', location_supports_sport, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Location Supports Sport', new_location_supports_sport_path %>
<%= will_paginate @location_supports_sports %>
